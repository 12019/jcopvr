TARGETNAME=jcop_proxy
TARGETTYPE=PROGRAM
TARGETPATH=obj
TARGETLIBS=$(DDK_LIB_PATH)\ws2_32.lib

INCLUDES=..\inc

# Make sure we are building a multi-threaded app
!IF "$(NTDEBUG)" == "retail" || "$(NTDEBUG)" == "" || "$(NTDEBUG)" == "ntsdnodbg"
#...release
CFLAGS=$(CFLAGS) /MT
!ELSE
#...debug
CFLAGS=$(CFLAGS) /MTd
!ENDIF


SOURCES=jcop_proxy.c	\
	jcop_simul.cpp	\
	dbglog.cpp

UMTYPE=console
UMBASE=0x0400000
